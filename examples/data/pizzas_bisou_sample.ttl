    pizza:has_kind pizza:_porkPizza ;
    pizza:has_topping pizza:_blackPepper,
        pizza:_cherryTomato,
        pizza:_mozzaDiBurrata,
        pizza:_mozzaFiorDiLatte,
        pizza:_oliveOil,
        pizza:_parmaHam,
        pizza:_parmesan,
        pizza:_rocket .

