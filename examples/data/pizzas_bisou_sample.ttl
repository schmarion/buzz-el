pizza:_biancaCastafiore rdfs:label "Bianca Castafiore"@en,  
        "Bianca Castafiore"@fr .
pizza:_biancaCastafiore skos:altLabel "Castafiore"@en,
        "Castafiore"@fr .

pizza:_biancaCastafiore pizza:has_base pizza:_ricottaCreamBase ;
    pizza:has_kind pizza:_goatCheesePizza,
        pizza:_vegetarianPizza ;
    pizza:has_topping pizza:_blackPepper,
        pizza:_goatCheese,
        pizza:_honey,
        pizza:_mozzaFiorDiLatte,
        pizza:_spinach,
        pizza:_walnut .

